package com.qa.abstractclass;

public class TestPage {

	public static void main(String[] args) {

		LoginPage lp=new LoginPage();
		lp.header();
		lp.footer();
		lp.privacyPolicy();
		lp.logo();
		
		
		System.out.println("----------------");
		
		Page p = new LoginPage();
		p.footer();
		p.header();
		p.privacyPolicy();
		p.logo();

		System.out.println("----------------");

		Page p2 = new HomePage();
		p2.footer();
		p2.header();
		p2.privacyPolicy();
		p2.logo();
	}

}
